{
  "manifest_version": 3,
  "name": "C418",
  "version": "1.0",
  "description": "Opens the Embedded Player in a new tab",
  "action": {
    "default_title": "Open Embedded Player"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs"
  ],
  "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }  
}
